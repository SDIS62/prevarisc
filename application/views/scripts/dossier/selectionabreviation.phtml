<?php
//Page utilise uniquement pour l'auto compltion. Retourne infos en json
$q = strtolower($_GET["q"]);
if(!$q)
	return;

$items = array();
foreach($this->selectAbreviation as $abreviation)
{
	$id = $abreviation['ID_PRESCRIPTIONTYPE'];
	$abreviation = $abreviation['ABREVIATION_PRESCRIPTIONTYPE'];		
	array_push($items, array(
		"id" => $id,
		"infos" => $abreviation
	));
}
echo json_encode($items);
?>