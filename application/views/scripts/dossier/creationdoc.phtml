function addChamp($odf,$nomOdf, $variable){
  try{
    if($variable == "") {
      $variable = "";
    }
    $odf->setVars($nomOdf,$variable, true,'UTF8');
    #$segment->$nomOdf(utf8_decode($variable));
  } catch (OdfException $e) {
  
  }
}

