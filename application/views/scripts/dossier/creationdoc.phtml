// PARTIE PRESCRIPTION
try{
    $nbPrescDossier = count($this->prescriptionDossier);
    $prescription = $odf->setSegment('listePrescription');
    for($i=0; $i < $nbPrescDossier; $i++)
    {
		$nbAssoc = count($this->prescriptionDossier[$i]);
		$cpt = true;
		$article = '';
		$texte = '';
		$textearticle = '';
		for($j=0; $j < $nbAssoc; $j++)
		{
			$articleT = $this->prescriptionDossier[$i][$j]['LIBELLE_ARTICLE'];
			$texteT = $this->prescriptionDossier[$i][$j]['LIBELLE_TEXTE'];

			if($texteT != ''){
				$textearticle .= $texteT." - ";
			}else{
				$textearticle .= " - ";
			}
			if($nbAssoc > 1){
				if($articleT != ''){
					$textearticle .= $articleT.", ";
				}else{
					$textearticle .= ", ";
				}
			}else{
				if($articleT != ''){
					$textearticle .= $articleT." ";
				}else{
					$textearticle .= " ";
				}
			}
			if($cpt)
			{
				addChamp($prescription, 'num', $this->prescriptionDossier[$i][0]['NUM_PRESCRIPTION_DOSSIER']);
				if( $this->prescriptionDossier[$i][0]['LIBELLE_PRESCRIPTION_DOSSIER'] == '')
				{
					addChamp($prescription, 'libelle', $this->prescriptionDossier[$i][0]['PRESCRIPTIONTYPE_LIBELLE']);
				}else{
					addChamp($prescription, 'libelle', $this->prescriptionDossier[$i][0]['LIBELLE_PRESCRIPTION_DOSSIER']);
				}
				$cpt = false;
			}
			$nbAssoc--;
		}
		addChamp($prescription, 'textearticle',$textearticle);
		$prescription->merge();
    }
    $odf->mergeSegment($prescription);	
} catch (OdfException $e) {
  
}

